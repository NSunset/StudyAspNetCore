
@{
    Layout = null;
}
@using Nw.LiveBackgroundManagement.Models.ViewModel
@model List<CSUserApplyViewModel>

<div class="timeline">
    @if (Model != null)
    {
        foreach (CSUserApplyViewModel apply in Model)
        {
            <div class="timeline-item">
                <div class="timeline-badge" style="padding-right: 0px">
                    <span class="label label-sm label-success">@apply.StrLastModifyTime</span>
                    <span class="label label-sm label-success">@apply.StateMessage</span>
                </div>
                <div class="timeline-body">
                    <div class="timeline-body-head">
                        <div class="timeline-body-head-caption">
                            @apply.ApprovalMsg
                        </div>
                        <div class="timeline-body-head-actions"> </div>
                    </div>
                </div>
            </div>
        }
    }
</div>
